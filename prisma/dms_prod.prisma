  updated_at         DateTime? @db.Timestamp(0)
}

model dm_tte_fields {
  id            BigInt    @id @default(autoincrement())
  ledger_id     Int
  field_name    String    @db.VarChar(255)
  field_type    String    @db.VarChar(255)
  default_value String?   @db.VarChar(700)
  status        Boolean   @default(false)
  created_by    BigInt
  updated_by    BigInt
